extend type Query {
    salesmanSets: [SalesmanSet]
}

type Distance {
    startCoordinateId: String
    endCoordinateId: String
    value: Float
}

type SalesmanSet {
    id: ID!
    places: [Coordinate]
    paths: [Path]
    neighborhoodMatrix: [Distance]
}

scalar ObjectScalar

extend type Mutation {
    newSalesmanSet(coordinates: [CoordinateInput]) : SalesmanSet!
    deleteSalesmanSet(id: ID!) : Boolean
}

input CoordinateInput {
    id: ID,
    location: [Float]
}
