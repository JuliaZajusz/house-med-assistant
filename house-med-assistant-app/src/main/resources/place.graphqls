type Query {
    places: [Place]
    coordinatePlaces(coordinateId: ID!): [Place]
    findPlaceByName(name: String): [Place]
}

type Place {
    id: ID!
    name: String
    coordinate: Coordinate
}

type Mutation {
    newPlace(name: String, location: [Float]) : Place!
    deletePlace(id: ID!) : Boolean
    #    updatePlace(id:ID!, location: [Float]) : Place!
}

