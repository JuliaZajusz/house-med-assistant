extend type Query {
    paths: [Path]
}

type Path {
    id: ID!
    places: [Coordinate]
}

extend type Mutation {
    newPath(places: [CoordinateInput]) : Path!
    deletePath(id: ID!) : Boolean
    #    updatePath(id:ID!, location: [Float]) : Path!
}

input CoordinateInput {
    id: ID,
    location: [Float]
}
